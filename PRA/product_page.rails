	<!-- Hide the inclusion table if there are no products in it -->
	<div data-repeat="family in SBQQ__Quote_Lines__r" data-group-by="SBQQ__Quote__c" data-filter="SBQQ__Hidden__c == 'false' AND SBQQ__Optional__c == 'false'"><!-- Inclusion table v2 --><hr />
	<h3><strong>{{Name_External__c}} Inclusions</strong></h3>
	<table style="width: 100%;">
		<thead>
			<tr>
				<td style="width: 85%;"></td>
				<td style="width: 15%; text-align: center;">Quantity</td>
			</tr>
		</thead>
		<!-- Loops through products associated with the header bundle and displays the family name -->
		<tbody data-repeat="family in SBQQ__Quote_Lines__r" data-filter="SBQQ__Hidden__c == 'false' AND SBQQ__Optional__c == 'false'" data-group-by="SBQQ__Product__r_Family">
			<tr>
				<td style="width: 85%;"><strong>[family.SBQQ__Product__r_Family]</strong></td>
				<td style="width: 15%; text-align: center;"></td>
			</tr>
			<!-- Loops through products associated with the family displayed in the row above and displays product name and quantity -->
			<tr data-repeat="product in SBQQ__Quote_Lines__r" data-filter="SBQQ__Product__r_Family == family.SBQQ__Product__r_Family AND SBQQ__Hidden__c != 'true'">
				<td style="width: 85%; padding-left: 30px;">[product.Name_External__c]<br /><span data-show-if="product.SBQQ__Optional__c == 'true'" style="font-size: 10px;"><em>Optional</em></span></td>
				<td style="width: 15%; text-align: center;" class="integer">[product.SBQQ__Quantity__c]</td>
			</tr>
			<!-- SUMs all the products associated with the current family -->
			<tr>
				<td style="width: 85%; padding-left: 30px;"><em>Total cost for [family.SBQQ__Product__r_Family] - <span class="money">[SUM(SBQQ__NetTotal__c)]</span></em></td>
				<td style="width: 15%; text-align: center;"></td>
			</tr>
		</tbody>
		<!-- Loops through all the products associated with the header bundle and displays that total -->
		<tfoot data-repeat="sum in SBQQ__Quote_Lines__r" data-filter="SBQQ__Hidden__c == 'false' AND SBQQ__Optional__c == 'false'" data-group-by="SBQQ__RequiredBy__c">
		<tr>
			<td style="width: 85%; background-color: #c4c4c4;"><strong>OVERALL TOTAL: <span class="money">[SUM(SBQQ__NetTotal__c)]</span></strong></td>
			<td style="width: 15%; background-color: #c4c4c4; text-align: center;"></td>
		</tr>
		</tfoot>
	</table>
