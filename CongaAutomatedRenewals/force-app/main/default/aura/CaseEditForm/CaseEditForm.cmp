<aura:component implements="forceCommunity:availableForAllPageTypes,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:lightningQuickAction,force:hasRecordId" access="global" >
	        
	    	<lightning:card title="">

		        <lightning:recordEditForm aura:id="edit" recordId="{!v.recordId}" recordTypeId="01250000000DzMY" objectApiName="Case">
		            <div aura:id="msg">
		                <lightning:messages />
		            </div>
		            <div class="slds-grid slds-wrap case-edit-form">
			    		<div class="slds-col slds-p-horizontal_medium slds-p-bottom_large slds-xx-small-size_12-of-12 slds-x-small-size_12-of-12 slds-medium-size_6-of-12 slds-large-size_6-of-12 ">
			    			<lightning:inputField fieldName="Subject" />
			    		</div>
			    		<div class="slds-col slds-p-horizontal_medium slds-p-bottom_large slds-xx-small-size_12-of-12 slds-x-small-size_12-of-12 slds-medium-size_6-of-12 slds-large-size_6-of-12">
			    			<lightning:inputField fieldName="Description" />
			    		</div>
			    		<div class="slds-col slds-p-horizontal_medium slds-p-bottom_large slds-xx-small-size_12-of-12 slds-x-small-size_12-of-12 slds-medium-size_6-of-12 slds-large-size_6-of-12">
			    			<lightning:inputField fieldName="Product__c" />
			    		</div>
			    		<div class="slds-col slds-p-horizontal_medium slds-p-bottom_large slds-xx-small-size_12-of-12 slds-x-small-size_12-of-12 slds-medium-size_6-of-12 slds-large-size_6-of-12">
			    			<lightning:inputField fieldName="Priority" />
			    		</div>
			    		<div class="slds-col slds-p-horizontal_medium slds-p-bottom_large slds-xx-small-size_12-of-12 slds-x-small-size_12-of-12 slds-medium-size_6-of-12 slds-large-size_6-of-12">
			    			<lightning:inputField onchange="{!c.checkValue}" aura:id="preferred_contact_method" fieldName="Preferred_Contact_Method__c" />
			    		</div>
			    		<div class="slds-col slds-p-horizontal_medium slds-p-bottom_large slds-xx-small-size_12-of-12 slds-x-small-size_12-of-12 slds-medium-size_6-of-12 slds-large-size_6-of-12">
			    			<lightning:inputField onchange="{!c.checkValue}" aura:id="other_contact_method" fieldName="Other_Contact_Method__c" />
			    		</div>
			    		<div class="slds-col slds-p-horizontal_medium slds-p-bottom_large slds-xx-small-size_12-of-12 slds-x-small-size_12-of-12 slds-medium-size_6-of-12 slds-large-size_6-of-12">
			    			<lightning:inputField fieldName="Org_ID__c" />
			    		</div>
			    		<div class="slds-col slds-p-horizontal_medium slds-p-bottom_large slds-xx-small-size_12-of-12 slds-x-small-size_12-of-12 slds-medium-size_6-of-12 slds-large-size_6-of-12">
			    		</div>
			    		<lightning:button aura:id="saveCaseUpdate" type="submit" label="Save" name="save" variant="brand" onclick="{!c.closeModal}" class="slds-m-top_small slds-m-left_medium" />
			    	</div>
		        </lightning:recordEditForm>
		        
		    </lightning:card>
    
</aura:component>