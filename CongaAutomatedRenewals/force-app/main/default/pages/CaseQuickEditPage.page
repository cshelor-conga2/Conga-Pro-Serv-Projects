<apex:page standardController="Case" extensions="CaseQuickEditController">
    <apex:form id="frm" >
        <apex:pageMessages ></apex:pageMessages>
        <apex:includeScript value="/support/console/20.0/integration.js"/>
        <script type="text/javascript">
            function go() { 
                sforce.console.openPrimaryTab(null,'/{!Case.Id}',true,'{!Case.CaseNumber}', openSuccess, 'CaseTab');
            }
            
            var openSuccess = function openSuccess(result){
            if(result.success == true){
                alert('Changes saved successfully.');
            }else{
                alert('Changes did not save successfully. Please try again.');
            }
        }
        
        </script>
        
           <!-- <A HREF="#" onClick="openPrimaryTab();return false">Open A Primary Tab</A> -->

        <apex:pageBlock title="Edit Fields" >
            <apex:pageBlockButtons >
                <apex:commandButton value="Save" action="{!Save}" rerender="frm" oncomplete="go();" />
            </apex:pageBlockButtons>
            <apex:pageblockSection columns="1">
                <apex:repeat value="{!$ObjectType.Case.FieldSets.Edit_Case_Fields}" var="cEdit">
                    <apex:inputField value="{!Case[cEdit]}"/>
                </apex:repeat>
            </apex:pageblockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>