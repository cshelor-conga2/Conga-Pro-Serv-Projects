<apex:page showHeader="false" sidebar="false" applyHtmlTag="false" applyBodyTag="false" docType="html-5.0" standardStyleSheets="false" controller="AccountContactUtilities">
<html lang="en">
        <head>
            <meta charset="utf-8"></meta>
            <meta http-equiv="X-UA-Compatible" content="IE=edge"></meta>
            <meta name="viewport" content="width=device-width, initial-scale=1"></meta>
            <meta name="description" content=""></meta>
            <meta name="author" content="James Hafer"></meta>

            <!-- Import the Design System style sheet -->
            <apex:slds />

            <title>Conga | Create Contact Roles</title>
            <link rel="icon" href="{!URLFOR($Resource.Favicon)}" type="image/x-icon" />
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" />
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css" />
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css" />
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/css/select2.min.css" />
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.css" />
            <link rel="stylesheet" href="https://unpkg.com/ng-table@2.0.2/bundles/ng-table.min.css" />
			<link rel="stylesheet" href="{!URLFOR($Resource.AccountContactTable, 'opportunityContactRole.css')}" />
			<!--<apex:stylesheet value="{!URLFOR($Resource.pi__font_awesome_4_2_0, 'font-awesome-4.2.0/css/font-awesome.min.css')}" />-->

		
            <style type="text/css">
                .modal {
                    display:    none;
                    position:   absolute;
                    z-index:    1999;
                    top:        0;
                    left:       0;
                    height:     100%;
                    width:      100%;
                    background: rgba( 255, 255, 255, .8 )
                                50% 50%
                                no-repeat;
                }

                #myPage.loading {
                    overflow: hidden;
                }

                #myPage.loading .modal {
                    display: block;
                }

                #modal-create-contact.loading{
                    overflow: hidden;
                }

                #modal-create-contact.loading{
                    overflow: hidden;
                }

                #modal-create-contact .modal {
                    display: block;
                }

                div .low-padding {
                    padding-right: 2px;
                    padding-left: 2px;
                }
				th.header{
					cursor: pointer;
				}
			</style>
			
        </head>
        <body id="ControllerAccountContactTable" ng-app="appAccountContactTable" ng-controller="ControllerAccountContactTable">
        <div id="myPage">
            <div class="slds-scope">
                <div style="position: fixed; top: 50%; left: 50%; z-index: 2000">
                    <div id="spinner" role="status" class="slds-spinner slds-spinner_large slds-hidden" style="z-index: 2001">
                        <span class="slds-assistive-text">Loading</span>
                        <div class="slds-spinner__dot-a"></div>
                        <div class="slds-spinner__dot-b"></div>
                    </div>
                </div>
            </div>

            <!-- Controller Properties - Begin -->
            <input type="hidden" id="opportunityId" value="{!opportunityId}" />
            <input type="hidden" id="accountId" value="{!accountId}" />
            <input type="hidden" id="accountContactRelationshipMaintenancePage" value="{!isForAccountContactRelationshipMaintenance}"/>
            <input type="hidden" id="opportunityContactRoleMaintenancePage" value="{!isForOpportunityContactRoleMaintenance}"/>
            <!-- Controller Properties - End -->

            <div id="alert-error" class="alert alert-danger hidden-conga" role="alert">
                <button id="close-alert-error" type="button" class="close" aria-label="Close"><span aria-hidden="true">×</span></button>
                <strong>Alert!&#32;</strong><span id="alert-error-message"></span>
            </div>
            <div id="alert-success" class="alert alert-success hidden-conga" role="alert">
                <button id="close-alert-success" type="button" class="close" aria-label="Close"><span aria-hidden="true">×</span></button>
                <span id="alert-success-message"></span>
            </div>
            <div class="modal fade" id="modal-create-contact" tabindex="-1" role="dialog">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="slds-scope">
                            <div id="spinner-modal" role="status" class="slds-spinner slds-spinner_large slds-hidden" style="z-index: 2001">
                                <span class="slds-assistive-text">Loading</span>
                                <div class="slds-spinner__dot-a"></div>
                                <div class="slds-spinner__dot-b"></div>
                            </div>
                        </div>
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">×</span>
                            </button>
                            <h3 class="modal-title subheading">New Contact</h3>
                        </div>
                        <div class="modal-body">
                            <form>
                                <fieldset>
                                    <div class="form-group-row sm-margin">
                                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                            <label for="contact-search">Search Existing Contacts:</label>
                                            <input type="text" id="contact-search" class="form-control" placeholder="Search Contacts" autofocus="true"/>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                            <label for="firstName">First Name:</label>
                                            <input id="firstName" class="form-control required" type="text" ng-model="contact.FirstName" required="true"></input>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                            <label for="lastName">Last Name:</label>
                                            <input id="lastName" class="form-control required" type="text" ng-model="contact.LastName" required="true"></input>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                            <label for="contatTitle">Title:</label>
                                            <input id="contatTitle" class="form-control" type="text" ng-model="contact.Title"></input>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                            <label for="contactJobLevel">Job Level:</label>
                                            <select class="form-control" ng-options="option as option for option in contactJobLevel" ng-model="JobLevel" id="contactJobLevel"></select>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                            <label for="email">Email:</label>
                                            <input id="email" class="form-control required" type="email" ng-model="contact.Email" required="true"></input>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                            <label for="phone">Phone:</label>
                                            <input class="form-control" type="phone" ng-model="contact.Phone"></input>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                            <label for="contactRole">Role:</label>
                                            <select class="form-control js-example-basic-multiple" ng-options="option as option for option in contactRoles" ng-model="roles" id="contactRole" multiple="true" style="width:100%"/>
                                        </div>
                                        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                            <label for="contactPersona">Persona:</label>
                                            <select class="form-control" ng-options="option as option for option in contactPersona" ng-model="Persona" id="contactPersona"></select>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                                            <label for="contactStatus">Status:</label>
                                            <select class="form-control" ng-options="option as option for option in contactStatus" ng-model="Status" id="contactStatus"></select>
                                        </div>
                                    </div>
                                </fieldset>
                            </form>
                        </div>
                        <div class="modal-footer">
                            <i id="fa-loader-create-contact" class="fa fa-spinner fa-pulse fa-3x fa-fw"></i>
                            <button id="addNewButton" type="button" class="btn btn-success" ng-click="insertContact(true)">Add New</button>
                            <button id="saveChangesButton" type="button" class="btn btn-primary" ng-click="insertContact(false)">Save Changes</button>
                            <button type="button" class="btn" data-dismiss="modal" aria-label="Close">Cancel</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="jumbotron">
                <p class="container">
                    <img id="img-logo-header" src="{!URLFOR($Resource.congaLogo)}" class="img-responsive" />
                </p>
            </div>
            <!-- PAGE MESSAGES -->
            <apex:pageMessages />
            <!-- PAGE MESSAGES -->
            <apex:outputPanel styleclass="container marketing" layout="block" style="width: 96%; text-align: center;" rendered="{!isValid}">
                <apex:outputPanel layout="block" rendered="{!isForOpportunityContactRoleMaintenance}">
                    <h2 class="conga-red-header">Opportunity Contact Management</h2>
                    <h4>
                        <ol style="text-align: left">
                            <li><b>Add</b> or <b>Modify</b> contact relationships on this screen</li><br></br>
                            <!--<li>Use the checkboxes on the left to select who will be <b>added</b> to this opportunity</li><br></br>-->
                            <li>Use the checkboxes on the left to select all contacts who have been involved from the start of this opportunity and identify the role(s) they played.</li><br></br>
                            <li>Select a <b>Primary Contact</b> on the right (a Primary Contact must also have a <b>Role</b>)</li><br></br>
                            <li>Use the <b>Add Contact</b> button to create new contacts for this account or add contacts from other accounts</li><br></br>
                            <li>Click the <b>Save</b> button</li><br></br>
                        </ol>
                    </h4>
                </apex:outputPanel>
                <apex:outputPanel styleclass="container marketing" layout="block" style="width: 96%; text-align: center;" rendered="{!isForAccountContactRelationshipMaintenance}">             
                    <h2 class="conga-red-header">Account Contact Management</h2>
                    <h4>
                        <ol style="text-align: left">
                            <li><b>Add</b> or <b>Modify</b> contact relationships on this screen</li><br></br>
                            <li>Use the <b>‘Add Contact’</b> button to create new contacts for this account or add contacts from other accounts</li><br></br>
                            <li>Click the <b>Save</b> button</li><br></br>
                        </ol>
                    </h4>
                    <h5 class="conga-red-header italic-font"><i>NOTE: If a contact has departed, <b>uncheck</b> them as Primary and set their status to <b>‘No Longer at Company’</b></i></h5>
                </apex:outputPanel>
                <hr class="featurette-divider"></hr>
