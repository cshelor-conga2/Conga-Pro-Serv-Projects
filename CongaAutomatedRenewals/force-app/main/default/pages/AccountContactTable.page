                <script id="selectAllHeader" type="text/ng-template">
                    <label data-toggle="tooltip" data-placement="top" title="Select All">Select All
                        <input type="checkbox" id="selectAll" ng-click="selectAll()"></input>
                    </label>
                </script>
                <div class="input-append">
					<i class="fa fa-search"></i>
					<input type="search" style="width: 300px" ng-model="searchText" placeholder="Search"/>
                	<!--<span class="add-on"><i class="icon-search"></i></span>-->
                </div>
                <table ng-table="tableParams" class="table" show-filter="true">
                    <thead>
                        <tr>
                            <th class="Id" >
                                <span ng-show="false">Id</span>
							</th>
							<th align = "left" ng-if="{!isForOpportunityContactRoleMaintenance}">
								<label data-toggle="tooltip" data-placement="top" title="Select All">Select All
									<input type="checkbox" id="selectAll" ng-click="selectAll()"></input>
								</label>
							</th>
                            <th class="name header"  ng-click="sort_by('Name')">Name
                                <span ng-show="sortingOrder == 'Name' && !reverse" class="fa fa-sort-amount-asc"></span>
                                <span ng-show="sortingOrder == 'Name' && reverse" class="fa fa-sort-amount-desc"></span>
                            </th>
                            <th class="title header" ng-click="sort_by('Title')">Title
                                <span ng-show="sortingOrder == 'Title' && !reverse" class="fa fa-sort-amount-asc"></span>
                                <span ng-show="sortingOrder == 'Title' && reverse" class="fa fa-sort-amount-desc"></span>
                            </th>
                            <th class="jobLevel header" ng-click="sort_by('JobLevel')">Job Level
                                <span ng-show="sortingOrder == 'JobLevel' && !reverse" class="fa fa-sort-amount-asc"></span>
                                <span ng-show="sortingOrder == 'JobLevel' && reverse" class="fa fa-sort-amount-desc"></span>
                            </th>
                            <th class="email header" ng-click="sort_by('Email')">Email
                                <span ng-show="sortingOrder == 'Email' && !reverse" class="fa fa-sort-amount-asc"></span>
                                <span ng-show="sortingOrder == 'Email' && reverse" class="fa fa-sort-amount-desc"></span>
                            </th>
                            <th class="persona header" ng-click="sort_by('Persona')">Persona
                                <span ng-show="sortingOrder == 'Persona' && !reverse" class="fa fa-sort-amount-asc"></span>
                                <span ng-show="sortingOrder == 'Persona' && reverse" class="fa fa-sort-amount-desc"></span>
                            </th>
                            <th class="status header" ng-click="sort_by('Status')">Status
                                <span ng-show="sortingOrder == 'Status' && !reverse" class="fa fa-sort-amount-asc"></span>
                                <span ng-show="sortingOrder == 'Status' && reverse" class="fa fa-sort-amount-desc"></span>
                            </th>
							<th class="role header" ng-click="sort_by('Role')">Role
                                <span ng-show="sortingOrder == 'Role' && !reverse" class="fa fa-sort-amount-asc"></span>
                                <span ng-show="sortingOrder == 'Role' && reverse" class="fa fa-sort-amount-desc"></span>
                            </th>
							<th class="primaryOpp header" ng-click="sort_by('Opp')" ng-if="{!isForOpportunityContactRoleMaintenance}">Primary for Opportunity
                                <span ng-show="sortingOrder == 'Opp' && !reverse" class="fa fa-sort-amount-asc"></span>
                                <span ng-show="sortingOrder == 'Opp' && reverse" class="fa fa-sort-amount-desc"></span>
                            </th>
                            <th class="primary header" ng-click="sort_by('IsPrimaryAccountRelation')">Primary for Account
                                <span ng-show="sortingOrder == 'IsPrimaryAccountRelation' && !reverse" class="fa fa-sort-amount-asc"></span>
                                <span ng-show="sortingOrder == 'IsPrimaryAccountRelation' && reverse" class="fa fa-sort-amount-desc"></span>
                            </th>
                        </tr>
                    </thead>
                    <tr ng-repeat="record in $data | orderBy:sortingOrder:reverse | filter:searchText | filter : statusFilter()">
                        <td>
                            <span ng-show="false" ng-bind="record.Id"></span>
                        </td>
                        <td title="'Select'" header="'selectAllHeader'" header-title="'The main point(s) of contact for this account.'" ng-if="{!isForOpportunityContactRoleMaintenance}">
                            <div class="low-padding">
                                <input type="checkbox" ng-model="record.Selected" ng-change="setChanged({{record.primaryOppRoleIndex}})"/>
                            </div>
                        </td>
                        <td title="'Name'"  header-title="'The main point(s) of contact for this account.'" >
                            <div class="contactNameContainer low-padding" style="text-align: left">
                                <h5 ng-bind-html="record.Name" ng-model="record.Name" class="contactName"></h5>
                            </div>
                        </td>
                        <td title="'Title'" header-title="'The main point(s) of contact for this account.'" >
                            <div class="low-padding" style="text-align: left">
                                <h5 ng-bind-html="record.Title" ng-model="record.Title" class="contactName"></h5>
                            </div>
                        </td>
                        <td title="'Job Level'" header-title="'Contact’s authority at company (closest approximation).'" >
                            <div class="low-padding">
                                <select class="form-control" ng-options="option as option for option in contactJobLevel" ng-model="record.JobLevel" ng-change="setChanged({{record.primaryOppRoleIndex}})"/>
                            </div>
                        </td>
                        <td title="'Email'" header-title="'The main point(s) of contact for this account.'" >
                            <div class="low-padding">
                                <input type="email" ng-bind-html="record.Email" ng-model="record.Email" class="form-control" ng-change="setChanged({{record.primaryOppRoleIndex}})"></input>
                            </div>
                        </td>
                        <td title="'Persona'" header-title="'Contact’s role at company (what they do at their company).'" >
                            <div class="low-padding">
                                <select class="form-control" ng-options="option as option for option in contactPersona" ng-model="record.Persona" ng-change="setChanged({{record.primaryOppRoleIndex}})"/>
                            </div>
                        </td>
                        <td title="'Status'" header-title="'Used to describe the current state of communication between Conga and the contact.'">
                            <div class="low-padding">
                                <select class="form-control" ng-options="option as option for option in contactStatus" ng-model="record.Status" ng-change="setChanged({{record.primaryOppRoleIndex}})"/>
                            </div>
                        </td>
                        <td title="'Role'" header-title="'Contact’s role in relation to Conga (how Conga works with them)'" >
                            <div class="low-padding">
                                <select class="form-control js-example-basic-multiple" ng-options="option as option for option in contactRoles" ng-model="record.Role" multiple="true" ng-change="setChanged({{record.primaryOppRoleIndex}})"/>
                            </div>
                        </td>
                        <td title="'Primary for Opportunity'" header-title="'The main point of contact for this opportunity. One per opportunity.'" ng-if="{!isForOpportunityContactRoleMaintenance}">
                            <div class="low-padding" style="text-align: center">
                                <input type="radio" name="primaryForOpp" ng-value="{{record.primaryOppRoleIndex}}" ng-model="$parent.primaryOppRoleSelected" id="primaryOppRole-{{record.primaryOppRoleIndex}}" ng-change="setAccountPrimary({{record.primaryOppRoleIndex}})"/>
                            </div>
                        </td>
                        <td title="'Primary for Account'" header-title="'The main point(s) of contact for this account.'" >
                            <div class="low-padding" style="text-align: center">
                                <input type="checkbox" ng-disabled="{!isForOpportunityContactRoleMaintenance}" ng-model="record.IsPrimaryAccountRelation" ng-change="setChanged({{record.primaryOppRoleIndex}})"/>
                            </div>
                        </td>
                    </tr>
                </table>
                <i id="fa-loader-process-changes" class="fa fa-spinner fa-pulse fa-3x fa-fw"></i>
                <apex:outputPanel layout="block" rendered="{!isForOpportunityContactRoleMaintenance}">
                    <h5 class="conga-red-header italic-font">NOTE: These relationship designations will carry over to the associated account</h5>
                </apex:outputPanel>
                <div class="pull-right" id="buttonContainer">
                    <button role="button" class="btn btn-success" id="addContact" ng-click="addContact()">Add Contact</button>
                    <button role="button" class="btn btn-primary" id="submitSave" ng-click="saveContactRoles()">Save</button>
                    <button role="button" class="btn btn-secodary" id="cancel" ng-click="cancel()">Close</button>
                </div>
            </apex:outputPanel>
            <footer class="footer">
                <p class="pull-right">
